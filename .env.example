# Example environment file for TalentScout AI
# Copy to .env and fill values. Do NOT commit real secrets.

# AI Configuration
GROQ_API_KEY=
AI_MODEL=llama-3.1-8b-instant
MAX_TOKENS=2048
TEMPERATURE=0.7
REPETITION_PENALTY=1.2



# Database Configuration
DATABASE_URL=postgresql://postgres:changeme@localhost:5432/talentscout
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=talentscout

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
SECRET_KEY=change_me_in_production
SESSION_TIMEOUT=3600

# UI Configuration
DEFAULT_THEME=light
ENABLE_MULTIPLE_THEMES=true
